{
    "version": 2,
    "builds": [
      {
        "src": "server.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/otp/getRoom",
        "dest": "server.ts"
      },
      {
        "src": "/otp/otpVerify",
        "dest": "server.ts"
      },
      {
        "src": "/(.*)",
        "dest": "server.ts"
      }
    ]
  }